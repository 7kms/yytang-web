{
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
  "env": {
      "browser": true
  },
  "root": true,
  "parser": "babel-eslint",
  "extends": ["vue"],
  "plugins": ["flow-vars"],
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "indent": 0,
    "spaced-comment": 0
  }
}